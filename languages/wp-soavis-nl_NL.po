msgstr "De <code>slug</code> van het SoaVis type wat getoond moet worden. "

#: includes/class-wp-soavis-shortcodes.php:166
#@ wp-soavis
msgid "Valid values are: "
msgstr "Toegestane waarden zijn: "

#: includes/class-wp-soavis-shortcodes.php:167
#@ wp-soavis
msgid "<code>Service</code>, <code>Project</code>, <code>System</code>, <code>Chain</code>, <code>Component</code>. "
msgstr "<code>Service</code>, <code>Project</code>, <code>System</code>, <code>Chain</code>, <code>Component</code>. "

#: includes/class-wp-soavis-shortcodes.php:168
#@ wp-soavis
